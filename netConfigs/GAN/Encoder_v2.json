{ "NetworkName" : "BasicClassifier",
  "DefaultParams":{ "Filters":128,
                    "KernalSize":3,
                    "LatentSize":8,
                    "FCUnits":256},

  "NetworkStructure":{
    "Encoder":[
      { "layerType":"Conv2D",
        "layerName":"Conv2D_1",
        "layerInput":"input.image",
        "Parameters":{"filters":64,
                      "kernel_size":5,
                      "strides":2,
                      "padding":"same"}},
      { "layerType":"BatchNormalization",
        "layerName":"BatchNorm1",
        "layerInput":"Conv2D_1",
        "Parameters":{}},
      { "layerType":"LeakyRelu",
        "layerName":"LeakyRelu1",
        "layerInput":"BatchNorm1",
        "Parameters":{}},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_2",
        "layerInput":"LeakyRelu1",
        "Parameters":{"filters":128,
                      "kernel_size":5,
                      "strides":2,
                      "activation":"relu"}},
      { "layerType":"BatchNormalization",
        "layerName":"BatchNorm2",
        "layerInput":"Conv2D_2",
        "Parameters":{}},
      { "layerType":"LeakyRelu",
        "layerName":"LeakyRelu2",
        "layerInput":"BatchNorm2",
        "Parameters":{}},
      { "layerType":"Flatten",
        "layerName":"Flatten",
        "layerInput":"LeakyRelu2"},
      { "layerType":"Dense",
        "layerName":"Dense1",
        "layerInput":"Flatten",
        "Parameters":{"units":"FCUnits"}},
      { "layerType":"LeakyRelu",
        "layerName":"LeakyRelu3",
        "layerInput":"Dense1",
        "Parameters":{}},
      { "layerType":"Dense",
        "layerName":"Latent",
        "layerInput":"LeakyRelu3",
        "Output":true,
        "Parameters":{"units":"LatentSize",
                      "activation":"tanh"}}

    ]
  }
}
