{ "NetworkName" : "BasicClassifier",
  "DefaultParams":{ "LatentSize":8,
                    "Filters":128,
                    "KernalSize":3,
                    "FCUnits":256},

  "NetworkStructure":{
    "Encoder":[
      { "layerType":"Conv2D",
        "layerName":"Conv2D_1",
        "layerInput":"input.image",
        "Parameters":{"filters":"Filters//4",
                      "kernel_size":"KernalSize*2",
                      "strides":2,
                      "padding":"same",
                      "activation":"relu"}},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_2",
        "layerInput":"Conv2D_1",
        "Parameters":{"filters":"Filters//2",
                      "kernel_size":"KernalSize",
                      "strides":1,
                      "activation":"relu"}},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_3",
        "layerInput":"Conv2D_2",
        "Parameters":{"filters":"Filters",
                      "kernel_size":"KernalSize",
                      "strides":1,
                      "activation":"relu"}},
      { "layerType":"Flatten",
        "layerName":"Flatten",
        "layerInput":"Conv2D_3"},
      { "layerType":"Dense",
        "layerName":"Dense1",
        "layerInput":"Flatten",
        "Parameters":{"units":"FCUnits",
                      "activation":"relu"}},
      { "layerType":"Dense",
        "layerName":"Mu",
        "layerInput":"Dense1",
        "Parameters":{"units":"LatentSize",
                      "activation":null}},
      { "layerType":"Dense",
        "layerName":"Sigma",
        "layerInput":"Dense1",
        "Parameters":{"units":"LatentSize",
                      "activation":null}},
      { "layerType":"Sampling",
        "layerName":"Latent",
        "layerInput":["Mu","Sigma"],
        "Parameters":{}}
    ],
    "Decoder":[
      { "layerType":"Dense",
        "layerName":"DenseDecoder",
        "layerInput":"Latent",
        "Parameters":{"units":"FCUnits",
                      "activation":"relu"}},
      { "layerType":"Dense",
        "layerName":"DenseDecoder",
        "layerInput":"Latent",
        "Parameters":{"units":256,
                      "activation":"relu"}},
      { "layerType":"Reshape",
        "layerName":"DecoderReshape",
        "Parameters":{"target_shape":[4,4,16]},
        "layerInput":"DenseDecoder"},
      { "layerType":"Conv2DTranspose",
        "layerName":"Decoder_Conv1",
        "layerInput":"DecoderReshape",
        "Parameters":{"filters":128,
                      "kernel_size":5,
                      "strides":2,
                      "activation":"relu"}},
                      { "layerType":"Conv2DTranspose",
        "layerName":"Decoder_Conv2",
        "layerInput":"Decoder_Conv1",
        "Parameters":{"filters":128,
                      "kernel_size":5,
                      "strides":2,
                      "activation":"relu"}},
      { "layerType":"Conv2DTranspose",
        "layerName":"Decoder_Conv3",
        "layerInput":"Decoder_Conv2",
        "Parameters":{"filters":64,
                      "kernel_size":3,
                      "strides":1,
                      "activation":"relu"}},
      { "layerType":"Conv2DTranspose",
        "layerName":"Decoder_Conv4",
        "layerInput":"Decoder_Conv3",
        "Parameters":{"filters":1,
                      "kernel_size":3,
                      "strides":1,
                      "activation":null}},
      { "layerType":"CentralCropping2D",
        "layerName":"Decoder",
        "Output":true,
        "layerInput":"Decoder_Conv4",
        "Parameters":{"cropping":[28,28]}}
    ]
  }
}
