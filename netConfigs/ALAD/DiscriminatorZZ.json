{ "NetworkName" : "BasicClassifier",
  "DefaultParams":{ "Filters":128,
                    "KernalSize":3,
                    "FCUnits":256},

  "NetworkStructure":{
    "Encoder":[
      { "layerType":"Concatenate",
        "layerName":"LatentCombine",
        "layerInput":["input.z","input.zRec"],
        "Parameters":{"axis":-1}},
      { "layerType":"Dense",
        "layerName":"Dense1",
        "layerInput":"LatentCombine",
        "Parameters":{"units":"FCUnits",
                      "activation":"relu"}},
      { "layerType":"Dense",
        "layerName":"Dense2",
        "layerInput":"Dense1",
        "Parameters":{"units":"FCUnits",
                      "activation":"relu"}},
      { "layerType":"Dense",
        "layerName":"Discrim",
        "layerInput":"Dense2",
        "Output":true,
        "Parameters":{"units":1,
                      "activation":"sigmoid"}}

    ]
  }
}
