{ "NetworkName" : "BasicClassifier",
  "DefaultParams":{ "LatentSize":8,
                    "DropoutRate":0.30,
                    "FCUnits":256},

  "NetworkStructure":{
    "Encoder":[
      { "layerType":"Conv2D",
        "layerName":"Conv2D_1",
        "layerInput":"input.image",
        "Parameters":{"filters":16,
                      "kernel_size":5,
                      "strides":2,
                      "padding":"same",
                      "activation":"relu"}},
      { "layerType":"Dropout",
        "layerName":"Dropout1",
        "layerInput":"Conv2D_1",
        "Parameters":{"rate":"DropoutRate"}},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_2",
        "layerInput":"Dropout1",
        "Parameters":{"filters":32,
                      "kernel_size":5,
                      "strides":2,
                      "activation":"relu"}},
      { "layerType":"Dropout",
        "layerName":"Dropout2",
        "layerInput":"Conv2D_2",
        "Parameters":{"rate":"DropoutRate"}},
      { "layerType":"Flatten",
        "layerName":"Flatten",
        "layerInput":"Conv2D_2"},
      { "layerType":"Dense",
        "layerName":"Dense1",
        "layerInput":"Flatten",
        "Parameters":{"units":"FCUnits",
                      "activation":"relu"}},
      { "layerType":"Dense",
        "layerName":"Latent",
        "Output":true,
        "layerInput":"Dense1",
        "Parameters":{"units":"LatentSize",
                      "activation":null}}
    ],
    "Decoder":[
      { "layerType":"Dense",
        "layerName":"DenseDecoder1",
        "layerInput":"Latent",
        "Parameters":{"units":"FCUnits"}},
      { "layerType":"BatchNormalization",
        "layerName":"BatchNorm1",
        "layerInput":"DenseDecoder1",
        "Parameters":{}},
      { "layerType":"LeakyRelu",
        "layerName":"LeakyRelu1",
        "layerInput":"BatchNorm1",
        "Parameters":{}},
      { "layerType":"Dense",
        "layerName":"DenseDecoder2",
        "layerInput":"LeakyRelu1",
        "Parameters":{"units":1568}},
      { "layerType":"BatchNormalization",
        "layerName":"BatchNorm2",
        "layerInput":"DenseDecoder2",
        "Parameters":{}},
      { "layerType":"LeakyRelu",
        "layerName":"LeakyRelu2",
        "layerInput":"BatchNorm2",
        "Parameters":{}},
      { "layerType":"Reshape",
        "layerName":"DecoderReshape",
        "Parameters":{"target_shape":[7,7,32]},
        "layerInput":"BatchNorm2"},
      { "layerType":"Conv2DTranspose",
        "layerName":"Decoder_Conv1",
        "layerInput":"DecoderReshape",
        "Parameters":{"filters":16,
                      "kernel_size":5,
                      "strides":2,
                      "padding":"same",
                      "use_bias":false}},
      { "layerType":"BatchNormalization",
        "layerName":"BatchNorm3",
        "layerInput":"Decoder_Conv1",
        "Parameters":{}},
      { "layerType":"LeakyRelu",
        "layerName":"LeakyRelu3",
        "layerInput":"BatchNorm3",
        "Parameters":{}},
      { "layerType":"Conv2DTranspose",
        "layerName":"Decoder_Conv2",
        "layerInput":"LeakyRelu3",
        "Parameters":{"filters":8,
                      "kernel_size":5,
                      "strides":2,
                      "padding":"same",
                      "use_bias":false}},
      { "layerType":"BatchNormalization",
        "layerName":"BatchNorm4",
        "layerInput":"Decoder_Conv2",
        "Parameters":{}},
      { "layerType":"LeakyRelu",
        "layerName":"LeakyRelu4",
        "layerInput":"BatchNorm4",
        "Parameters":{}},
      { "layerType":"Conv2DTranspose",
        "layerName":"Decoder",
        "layerInput":"LeakyRelu4",
        "Output":true,
        "Parameters":{"filters":3,
                      "kernel_size":5,
                      "strides":1,
                      "padding":"valid",
                      "use_bias":false,
                      "activation":"tanh"}}
    ]
  }
}
