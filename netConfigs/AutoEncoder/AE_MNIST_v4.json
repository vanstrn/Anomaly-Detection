{ "NetworkName" : "BasicClassifier",
  "DefaultParams":{ "LatentSize":8,
                    "DropoutRate":0.30,
                    "FCUnits":256},

  "NetworkStructure":{
    "Encoder":[
      { "layerType":"Flatten",
        "layerName":"Flatten",
        "layerInput":"input.image"},
      { "layerType":"Dense",
        "layerName":"Dense1",
        "layerInput":"Flatten",
        "Parameters":{"units":"FCUnits",
                      "activation":"relu"}},
      { "layerType":"Dropout",
        "layerName":"Dropout1",
        "layerInput":"Dense1",
        "Parameters":{"rate":"DropoutRate"}},
      { "layerType":"Dense",
        "layerName":"Dense2",
        "layerInput":"Dropout1",
        "Parameters":{"units":"FCUnits",
                      "activation":"relu"}},
      { "layerType":"Dropout",
        "layerName":"Dropout2",
        "layerInput":"Dense2",
        "Parameters":{"rate":"DropoutRate"}},
      { "layerType":"Dense",
        "layerName":"Latent",
        "Output":true,
        "layerInput":"Dense2",
        "Parameters":{"units":"LatentSize",
                      "activation":null}}
    ],
    "Decoder":[
      { "layerType":"Dense",
        "layerName":"DenseDecoder1",
        "layerInput":"Latent",
        "Parameters":{"units":256,
                      "activation":"relu"}},
      { "layerType":"Dense",
        "layerName":"DenseDecoder2",
        "layerInput":"Latent",
        "Parameters":{"units":512,
                      "activation":"relu"}},
      { "layerType":"Dense",
        "layerName":"DenseDecoder3",
        "layerInput":"DenseDecoder2",
        "Parameters":{"units":784,
                      "activation":"relu"}},
      { "layerType":"Reshape",
        "layerName":"Decoder",
        "Output":true,
        "Parameters":{"target_shape":[28,28,1]},
        "layerInput":"DenseDecoder2"},

  }
}
