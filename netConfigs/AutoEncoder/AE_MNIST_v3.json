{ "NetworkName" : "BasicClassifier",
  "DefaultParams":{ "LatentSize":8,
                    "DropoutRate":0.30,
                    "FCUnits":256,
                    "Filters":64},

  "NetworkStructure":{
    "Encoder":[
      { "layerType":"Flatten",
        "layerName":"Flatten",
        "layerInput":"input.image"},
      { "layerType":"Dense",
        "layerName":"Dense1",
        "layerInput":"Flatten",
        "Parameters":{"units":"FCUnits",
                      "activation":"relu"}},
      { "layerType":"Dropout",
        "layerName":"Dropout1",
        "layerInput":"Dense1",
        "Parameters":{"rate":"DropoutRate"}},
      { "layerType":"Dense",
        "layerName":"Dense2",
        "layerInput":"Dropout1",
        "Parameters":{"units":"FCUnits",
                      "activation":"relu"}},
      { "layerType":"Dropout",
        "layerName":"Dropout2",
        "layerInput":"Dense2",
        "Parameters":{"rate":"DropoutRate"}},
      { "layerType":"Dense",
        "layerName":"Latent",
        "Output":true,
        "layerInput":"Dense2",
        "Parameters":{"units":"LatentSize",
                      "activation":null}}
    ],
    "Decoder":[
      { "layerType":"Dense",
        "layerName":"DenseDecoder1",
        "layerInput":"Latent",
        "Parameters":{"units":"FCUnits",
                      "activation":"relu"}},
      { "layerType":"Dense",
        "layerName":"DenseDecoder2",
        "layerInput":"DenseDecoder1",
        "Parameters":{"units":256,
                      "activation":"relu"}},
      { "layerType":"Reshape",
        "layerName":"DecoderReshape",
        "Parameters":{"target_shape":[4,4,16]},
        "layerInput":"DenseDecoder2"},
      { "layerType":"Conv2DTranspose",
        "layerName":"Decoder_Conv1",
        "layerInput":"DecoderReshape",
        "Parameters":{"filters":"Filters",
                      "kernel_size":5,
                      "strides":2,
                      "activation":"relu"}},
                      { "layerType":"Conv2DTranspose",
        "layerName":"Decoder_Conv2",
        "layerInput":"Decoder_Conv1",
        "Parameters":{"filters":"Filters/2",
                      "kernel_size":5,
                      "strides":2,
                      "activation":"relu"}},
      { "layerType":"Conv2DTranspose",
        "layerName":"Decoder_Conv3",
        "layerInput":"Decoder_Conv2",
        "Parameters":{"filters":"Filters/4",
                      "kernel_size":3,
                      "strides":1,
                      "activation":"relu"}},
      { "layerType":"Conv2DTranspose",
        "layerName":"Decoder_Conv4",
        "layerInput":"Decoder_Conv3",
        "Parameters":{"filters":1,
                      "kernel_size":3,
                      "strides":1,
                      "activation":null}},
      { "layerType":"CentralCropping2D",
        "layerName":"Decoder",
        "Output":true,
        "layerInput":"Decoder_Conv4",
        "Parameters":{"cropping":[28,28]}}
    ]
  }
}
